				<ProtocolVersionMajor type="Integer" value="1"/>
				<ProtocolVersionMinor type="Integer" value="1"/>
			</ProtocolVersion>
			<TimeStamp type="DateTime" value="2012-04-27T08:12:21+00:00"/>
